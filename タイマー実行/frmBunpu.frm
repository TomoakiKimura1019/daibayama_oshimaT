Public Sub BunpuPLOT()
    Dim i As Integer
    Dim NAGASA As Integer
    Dim DeepMax As Integer
    Dim DayCo As Integer

    kou_ID = 2: type_ID = 1
    
    'çÏê}ì˙éûÇÃçƒê›íË
    If Bunpu.Bday(2) <> Z_Keisoku_Time Then
        Bunpu.Bday(1) = Bunpu.Bday(2): Bunpu.Bday(2) = Z_Keisoku_Time
        For i = 1 To 2
            SS = "çÏê}ì˙éû" & CStr(i)
            If Bunpu.Bday(i) = 0 Then
                Call WriteIni("ífñ ï™ïzê}ê›íË", SS, "", CurrentDIR & "åvë™ê›íË.ini")
            Else
                Call WriteIni("ífñ ï™ïzê}ê›íË", SS, Format$(Bunpu.Bday(i), "yyyy/mm/dd hh:nn:ss"), CurrentDIR & "åvë™ê›íË.ini")
            End If
        Next i
    End If
    
    'çÏê}
    X1 = 1400
    Y1 = 4400
    NAGASA = 4000
    DeepMax = -999
    For dan_ID = 1 To DanSet(kou_ID, 0).dan
        YMIN = Tbl(kou_ID, dan_ID, 1).deep
        YMAX = Tbl(kou_ID, dan_ID, Tbl(kou_ID, dan_ID, 0).ten).deep
        If DeepMax < (YMAX - YMIN) Then DeepMax = YMAX - YMIN
    Next dan_ID
    If DeepMax = 0 Then Exit Sub
    yscl = NAGASA / DeepMax
    
    For dan_ID = 1 To DanSet(kou_ID, 0).dan
        
        Set TARGETOBJECT = VSDraw1(dan_ID)
        TARGETOBJECT.Clear
    
        YMIN = Tbl(kou_ID, dan_ID, 1).deep
        YMAX = Tbl(kou_ID, dan_ID, Tbl(kou_ID, dan_ID, 0).ten).deep

        XGL = X1: YGL = Y1

        PENC = 1: Call PENJ(TARGETOBJECT, PENC)

        Call TITLE
        Call DFLAME

        VSDraw1(dan_ID).PenStyle = 0

        For DayCo = 1 To 2
            If Bunpu.Bday(DayCo) <> 0 Then
                If DayCo = 1 Then PENC = 3
                If DayCo = 2 Then PENC = 2
                Call PENJ(TARGETOBJECT, PENC)
                Call DPLOT(DayCo)
            End If
        Next DayCo
    Next dan_ID
    
    Set TARGETOBJECT = VSDraw1(0)
    VSDraw1(0).Clear
    PENC = 1: Call PENJ(TARGETOBJECT, PENC)
    VSDraw1(0).PenStyle = 0
    Call DHAN
End Sub

Private Sub DHAN()
    Dim i As Integer
    
    PY = VSDraw1(0).ScaleTop - 100
    PX = VSDraw1(0).ScaleWidth / 2 - 200: SIZ = 180: XOFF = 400: YOFF = 0
    SS = "ñ}ó·"
    Call KPUT(TARGETOBJECT, SS, PX, PY, SIZ, XOFF, YOFF, 0, 0)
    
    For i = 1 To 2
        If i = 1 Then PENC = 3
        If i = 2 Then PENC = 2
        Call PENJ(TARGETOBJECT, PENC)
        
        PX = 100: PY = VSDraw1(0).ScaleTop - 500 - 400 * (i - 1): Call MMM(TARGETOBJECT, PX, PY)
        PX = 450: PY = VSDraw1(0).ScaleTop - 500 - 400 * (i - 1): Call DDD(TARGETOBJECT, PX, PY)
    
        PX = 500
        PY = VSDraw1(0).ScaleTop - 500 - 400 * (i - 1) + 90
        If Bunpu.Bday(i) = 0 Then
            SS = "ÉfÅ[É^Ç»Çµ"
        Else
            SS = Format$(Bunpu.Bday(i), "yyyy/mm/dd hh:nn:ss")
        End If
        SIZ = 180: XOFF = 180: YOFF = 0
        Call KPUT(TARGETOBJECT, SS, PX, PY, SIZ, XOFF, YOFF, 0, 0)
    Next i
End Sub

Private Sub DFLAME()
    Dim AX As Single
    Dim PSCL As Single
    Dim INTLEN As Integer, TENLEN As Integer
    Dim YSCLMAX As Integer, YSCLMIN As Integer
    Dim ten As Integer
    Dim j As Integer

    SCL = 1800
    XMIN = Bunpu.Xscl * 5 * (-1)
    XMAX = Bunpu.Xscl * 5
    XBUNKATU = 10
    
    DSC = (XMAX - XMIN) / XBUNKATU
    XBAIRITU = SCL / (XMAX - XMIN)
    
    YSCLMAX = Len(Trim$(CStr(CInt(XMAX))))
    YSCLMIN = Len(Trim$(CStr(CInt(XMIN))))
    If YSCLMAX > YSCLMIN Then
        INTLEN = YSCLMAX
    Else
        INTLEN = YSCLMIN
    End If
    PANKFM = String$(INTLEN, "#")
    Mid$(PANKFM, Len(PANKFM), 1) = "0"
    
    ten = InStr(Trim$(str$(DSC)), ".")
    Select Case ten
        Case Is <> 0
            TENLEN = Len(Trim$(str$(DSC))) - ten
            PANKFM = PANKFM + "." + String$(TENLEN, "#")
            Mid$(PANKFM, Len(PANKFM), 1) = "0"
    End Select
    

    PENC = 1: Call PENJ(TARGETOBJECT, PENC)
    PX = XGL: PY = YGL: Call MMM(TARGETOBJECT, PX, PY)
    PX = XGL: PY = YGL - (YMAX - YMIN) * yscl: Call DDD(TARGETOBJECT, PX, PY)
    
    PSCL = XMIN
    For AX = XMIN To XMAX Step DSC
        PX = XGL + AX * XBAIRITU: PY = YGL: Call MMM(TARGETOBJECT, PX, PY)
        PX = XGL + AX * XBAIRITU: PY = YGL + 50: Call DDD(TARGETOBJECT, PX, PY)
        
        PY = PY + 170
        If AX = XMIN Or AX = 0 Or AX = XMAX Then
            If AX = XMIN Then PX = PX - 730 + (Len(Format$(XMIN, "@")) - 1) * 170 Else PX = PX - 400       ' 69 + TENLEN * 15
            PANK = Format$(Format(AX, PANKFM), "@@@@@")
            PANKSIZE = 180: Call AnkCsize(TARGETOBJECT, PANKSIZE)
            Call PPANK(TARGETOBJECT, PANK, PX, PY)
        End If
    Next AX
        
    PX = XGL + SCL / 2: PY = YGL: Call MMM(TARGETOBJECT, PX, PY)
    PX = XGL - SCL / 2: PY = YGL: Call DDD(TARGETOBJECT, PX, PY)
    
    For j = Tbl(kou_ID, dan_ID, 0).ten To 1 Step -1
        Select Case j
            Case 1
                PX = XGL + SCL / 2: PY = YGL - (Tbl(kou_ID, dan_ID, j).deep - YMIN) * yscl: Call MMM(TARGETOBJECT, PX, PY)
                PX = XGL - SCL / 2: PY = YGL - (Tbl(kou_ID, dan_ID, j).deep - YMIN) * yscl: Call DDD(TARGETOBJECT, PX, PY)
            Case Else
                PX = XGL + 60: PY = YGL - (Tbl(kou_ID, dan_ID, j).deep - YMIN) * yscl: Call MMM(TARGETOBJECT, PX, PY)
                PX = XGL - 60: PY = YGL - (Tbl(kou_ID, dan_ID, j).deep - YMIN) * yscl: Call DDD(TARGETOBJECT, PX, PY)
        End Select

        PX = XGL - SCL / 2 - 300
        PY = YGL - (Tbl(kou_ID, dan_ID, j).deep - YMIN) * yscl + 70
        PANK = Format$(Tbl(kou_ID, dan_ID, j).deep, "@@@")
        PANKSIZE = 180: Call AnkCsize(TARGETOBJECT, PANKSIZE)
        Call PPANK(TARGETOBJECT, PANK, PX, PY)
    Next j
    
    PX = XGL - SCL / 2 - 400: PY = YGL - (YMAX - YMIN) * yscl - 150: SIZ = 150: XOFF = 180: YOFF = 0
    SS = "ê[ìxm"
    Call KPUT(TARGETOBJECT, SS, PX, PY, SIZ, XOFF, YOFF, 0, 0)
End Sub
  
Private Sub TITLE()
    Dim j As Integer
 
    PY = YGL + 500: SIZ = 240 - 80: XOFF = 240: YOFF = 0
    SS = Trim$(DanSet(kou_ID, dan_ID).ti)
    j = LenB(StrConv(SS, vbFromUnicode))
    PX = XGL - j * 60
    Call KPUT(TARGETOBJECT, SS, PX, PY, SIZ, XOFF, YOFF, 0, 0)
    
    PY = YGL + 350: SIZ = 240 - 80: XOFF = 240: YOFF = 0
    SS = "(" & Trim$(kou(kou_ID, 1).Yu) & ")"
    j = LenB(StrConv(SS, vbFromUnicode))
    PX = XGL - j * 60
    Call KPUT(TARGETOBJECT, SS, PX, PY, SIZ, XOFF, YOFF, 0, 0)
    
End Sub

Private Sub DPLOT(DayCo As Integer)
    Dim dt2(1 To 50) As Double         'åvë™íl (TDSÉfÅ[É^Å|èâä˙íl)Å~åWêî
    Dim pdSNG As Single, pdDBL As Double, pdLNG As Long
    Dim po As Long
    Dim SW As Boolean
    Dim FILENAME As String
    Dim da As Date
    Dim f As Integer, L As String
    Dim i As Integer, FLDno As Integer
    
    FILENAME = KEISOKU.Data_path & Trim$(kou(kou_ID, 1).FILENAME) & "-" & CStr(dan_ID) & ".dat"
    i = FileCheck(FILENAME, "åvë™ÉfÅ[É^")
    If i = 0 Then Unload åvë™Form: End

    po = STARTpoint(FILENAME, kou_ID, Bunpu.Bday(DayCo))
    f = FreeFile
    Open FILENAME For Input Shared As #f
        Seek #f, po
        Do While Not (EOF(f))
            Line Input #f, L
            da = CDate(Mid$(L, 1, 19))
            If DateDiff("s", da, Bunpu.Bday(DayCo)) > 0 Then Exit Sub
            If DateDiff("s", da, Bunpu.Bday(DayCo)) = 0 Then
                
                For ten_ID = 1 To Tbl(kou_ID, dan_ID, 0).ten
                    FLDno = Tbl(kou_ID, dan_ID, ten_ID).FLD
                    If IsNumeric(Mid$(L, 20 + 8 * (FLDno - 1), 8)) = True Then
                        pdSNG = CSng(Mid$(L, 20 + 8 * (FLDno - 1), 8))
                    Else
                        pdSNG = 999999
                    End If

                    If Abs(pdSNG) >= 999999 Or Tbl(kou_ID, dan_ID, ten_ID).Syo = 999999 Then
                        dt2(ten_ID) = 999999
                    Else
                        If kou_ID = 1 Then Call SinsyukuCAL(dan_ID, ten_ID, da, pdSNG)
                        dt2(ten_ID) = (pdSNG - Tbl(kou_ID, dan_ID, ten_ID).Syo) * Tbl(kou_ID, dan_ID, ten_ID).Kei
                    End If
'''                    dt2(ten_ID) = pdDBL
                Next ten_ID
'''                Call KEISAN(da, kou_ID, dan_ID, type_ID, dt2())
                
                SW = False
                For i = Tbl(kou_ID, dan_ID, 0).ten To 1 Step -1
                    pdDBL = dt2(i)
                    If pdDBL >= 999999 Then
                        SW = False
                    Else
                        PX = XGL + pdDBL * XBAIRITU
                        PY = YGL - (Tbl(kou_ID, dan_ID, i).deep - YMIN) * yscl
                        
                        If SW = False Then
                            Call MMM(TARGETOBJECT, PX, PY)
                            SW = True
                        Else
                            Call DDD(TARGETOBJECT, PX, PY)
                        End If
                    End If
                Next i
            End If
        Loop
    Close #f
End Sub

